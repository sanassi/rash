then echo manger dans les fesses > respect.txt ;
fi
cat respect.txt
respect=mort
case $respect in
    dead)
    echo vive macron
    ;;
    mort)
    echo mais c est pas fini
    ;;
esac
for i in 3 2 1
do
    echo "$i"
done
